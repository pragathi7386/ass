#include <stdio.h>
int main() 
{
    int ch;
    printf("enter ch value");
    scanf("%c",&ch);
    if( (ch>='A')&&(ch<='Z') || (ch>='a')&&(ch<='z') )
        printf("Alphabet");
    else
    if( (ch>='0') && (ch<='9') )
      printf("digit");
    else
    printf("special charcter");
    return 0;
    
}
